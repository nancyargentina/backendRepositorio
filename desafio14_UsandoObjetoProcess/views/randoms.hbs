<main class="container">
    <h1 class="text-center text-uppercase"> {{cantidad}} Números al azar del 1 al 1000</h1>
    <table class="table table-dark table-hover table-sm align-middle w-auto mx-auto">
            <thead>
                <tr class="text-uppercase">
                    <th>Número</th>
                    <th>Apariciones</th>
                </tr>
            </thead>
            <tbody>
                {{#each data}}
                    <tr>
                        <td>{{@key}}</td>
                        <td>{{this}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
</main>